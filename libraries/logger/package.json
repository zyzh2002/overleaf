    "lint:fix": "eslint --fix .",
    "test:ci": "npm run test"
  },
  "dependencies": {
    "@google-cloud/logging-bunyan": "^3.2.0",
    "@overleaf/o-error": "*",
    "@sentry/node": "^6.13.2",
    "bunyan": "^1.8.14",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.2.0",
    "sandboxed-module": "^2.0.4",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0"
  },
  "peerDependencies": {
    "@overleaf/metrics": "*"
  }
}
